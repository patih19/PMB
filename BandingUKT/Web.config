<configuration>
    <connectionStrings>
        <!--========== SQL Server 2012 ==============-->
        <add name="DB" connectionString="Data Source=DELL\MSSQLSERVER12;Initial Catalog=ukt;User ID=sa; Password=Psw4"/>
        <add name="DB" connectionString="Data Source=DELL\MSSQLSERVER12;Initial Catalog=UntidarDb; User ID=sa; Password=Passsw4" />
