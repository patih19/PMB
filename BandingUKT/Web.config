        <add name="Tidar" connectionString="Data Source=localhost;Initial Catalog=UntidarDb;Integrated Security=True" />-->
    </connectionStrings>
    <!-- Tambahan I-->
    <system.web.extensions>
        <scripting>
            <scriptResourceHandler enableCompression="true" enableCaching="true" />
        </scripting>
    </system.web.extensions>
    <system.web>
        <webServices>
            <protocols>
                <add name="HttpGet"/>
                <add name="HttpPost"/>
            </protocols>
        </webServices>
        <customErrors mode="Off" />
        <!--  <sessionState mode="StateServer" stateConnectionString="tcpip=192.168.1.99:42424" timeout="20" /> -->
        <sessionState mode="InProc" timeout="20" />
        <compilation debug="true" targetFramework="4.0">
            <assemblies>
                <add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
                <add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
                <add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
            </assemblies>
        </compilation>
        <!--<trace enabled="true"/>-->
        <!-- <httpHandlers>
      <add verb="GET" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha"/>
    </httpHandlers> -->
        <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
            <controls>
                <add tagPrefix="ajaxToolkit" assembly="AjaxControlToolkit" namespace="AjaxControlToolkit" />
            </controls>
        </pages>
        <!-- IIS Default Upload set to 4 Mb -->
        <httpRuntime maxRequestLength="4096" executionTimeout="999999" />
    </system.web>
    <system.webServer>
        <validation validateIntegratedModeConfiguration="false" />
        <!-- set to false to disable -->
        <directoryBrowse enabled="true" />
        <defaultDocument>
            <files>
                <clear />
                <add value="Login.aspx" />
                <!-- <add value="off.aspx"/> -->
            </files>
        </defaultDocument>
        <handlers>
            <add name="CaptchaImage" verb="GET" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha" />
            <!-- Using CombineCriptsHandler as name of handler -->
            <add name="CombineScriptsHandler" verb="*" path="CombineScriptsHandler.axd" type="AjaxControlToolkit.CombineScriptsHandler,AjaxControlToolkit" />
        </handlers>
    </system.webServer>
    <location path="CaptchaImage.axd">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>
    <system.serviceModel>
        <bindings />
        <client />
        <extensions>
